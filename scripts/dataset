#!/bin/bash

ZIP_FILE="asl-npy-20-action-60fps-225feature-no-augment.zip"

# download the dataset using Kaggle API
kaggle datasets download -d jirenmaa/asl-npy-20-action-60fps-225feature-no-augment -p .

# extract the downloaded zip file to the parent directory
unzip -o "$ZIP_FILE" -d ./

# remove the zip file after extraction
rm "$ZIP_FILE"